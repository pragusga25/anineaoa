{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "tsBuildInfoFile": ".tsbuildinfo"
  },
  "exclude": ["node_modules", "**/*.e2e.ts", "cypress"],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/pages/index.testx.tsxx",
    "app/pages/index.tsxa",
    "app/pages/index.test.tsxa",
    "app/auth/pages/login.tsxa",
    "app/auth/pages/signup.tsx.bak"
  ]
}
